{
  "name": "meeting-preparation-automation",
  "version": "1.0.0",
  "description": "Google Calendar and N8N Meeting Preparation Automation System",
  "main": "index.js",
  "scripts": {
    "dev": "docker-compose up",
    "start": "docker-compose up -d",
    "stop": "docker-compose down",
    "logs": "docker-compose logs -f",
    "db:migrate": "npm run migrate:up",
    "migrate:up": "knex migrate:latest",
    "migrate:down": "knex migrate:rollback",
    "migrate:make": "knex migrate:make"
  },
  "keywords": [
    "n8n",
    "google-calendar",
    "automation",
    "meeting-preparation"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^16.3.1",
    "knex": "^3.0.1",
    "pg": "^8.11.3"
  },
  "devDependencies": {
    "@types/node": "^20.10.0"
  }
}