{
  "name": "Daily Meeting Sync",
  "nodes": [
    {
      "parameters": {
        "rule": {
          "interval": [
            {
              "field": "cronExpression",
              "expression": "0 6 * * *"
            }
          ]
        }
      },
      "id": "schedule-trigger",
      "name": "毎朝6時実行",
      "type": "n8n-nodes-base.scheduleTrigger",
      "typeVersion": 1.2,
      "position": [240, 300]
    },
    {
      "parameters": {
        "functionCode": "// Google Calendar API呼び出しの準備\nconst today = new Date();\nconst todayStart = new Date(today.getFullYear(), today.getMonth(), today.getDate());\nconst todayEnd = new Date(today.getFullYear(), today.getMonth(), today.getDate() + 1);\n\nreturn [{\n  json: {\n    timeMin: todayStart.toISOString(),\n    timeMax: todayEnd.toISOString(),\n    calendarId: 'primary',\n    singleEvents: true,\n    orderBy: 'startTime',\n    timestamp: new Date().toISOString()\n  }\n}];"
      },
      "id": "prepare-calendar-params",
      "name": "カレンダーパラメータ準備",
      "type": "n8n-nodes-base.function",
      "typeVersion": 1,
      "position": [440, 300]
    },
    {
      "parameters": {
        "method": "GET",
        "url": "=https://www.googleapis.com/calendar/v3/calendars/{{ $json.calendarId }}/events",
        "sendQuery": true,
        "queryParameters": {
          "parameters": [
            {
              "name": "timeMin",
              "value": "={{ $json.timeMin }}"
            },
            {
              "name": "timeMax",
              "value": "={{ $json.timeMax }}"
            },
            {
              "name": "singleEvents",
              "value": "={{ $json.singleEvents }}"
            },
            {
              "name": "orderBy",
              "value": "={{ $json.orderBy }}"
            }
          ]
        },
        "sendHeaders": true,
        "headerParameters": {
          "parameters": [
            {
              "name": "Authorization",
              "value": "=Bearer {{ $env.GOOGLE_ACCESS_TOKEN }}"
            }
          ]
        },
        "options": {}
      },
      "id": "get-calendar-events",
      "name": "Google Calendar取得",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 4.2,
      "position": [640, 300]
    }
  ],
  "connections": {
    "schedule-trigger": {
      "main": [
        [
          {
            "node": "prepare-calendar-params",
            "type": "main",
            "index": 0
          }
        ]
      ]
    }
  },
  "settings": {
    "executionOrder": "v1"
  },
  "active": true
}
